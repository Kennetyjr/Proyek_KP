<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">


<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity_AdminPasswordChange">

    <EditText
        android:id="@+id/txtprofileadminnama"
        android:layout_width="341dp"
        android:layout_height="55dp"
        android:layout_marginTop="80dp"
        android:ems="10"
        android:enabled="false"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/txtprofileidadmin"
        android:layout_width="336dp"
        android:layout_height="57dp"
        android:layout_marginTop="40dp"
        android:ems="10"
        android:enabled="false"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="@+id/txtprofileadminnama"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="@+id/txtprofileadminnama"
        app:layout_constraintTop_toBottomOf="@+id/txtprofileadminnama" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Profile Admin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/txtprofilepasswordadmin"
        android:layout_width="334dp"
        android:layout_height="59dp"
        android:layout_marginTop="28dp"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="@+id/txtprofileidadmin"
        app:layout_constraintStart_toStartOf="@+id/txtprofileidadmin"
        app:layout_constraintTop_toBottomOf="@+id/txtprofileidadmin" />

    <EditText
        android:id="@+id/txtprofilenotelpadmin"
        android:layout_width="330dp"
        android:layout_height="62dp"
        android:layout_marginTop="40dp"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="@+id/txtprofilepasswordadmin"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/txtprofilepasswordadmin"
        app:layout_constraintTop_toBottomOf="@+id/txtprofilepasswordadmin" />

    <Button
        android:id="@+id/btnkonfirmasipasswordadmin"
        android:layout_width="176dp"
        android:layout_height="59dp"
        android:layout_marginTop="28dp"
        android:text="Okay"
        app:layout_constraintEnd_toEndOf="@+id/txtprofilenotelpadmin"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintStart_toStartOf="@+id/txtprofilenotelpadmin"
        app:layout_constraintTop_toBottomOf="@+id/txtprofilenotelpadmin" />
</androidx.constraintlayout.widget.ConstraintLayout>
</layout>